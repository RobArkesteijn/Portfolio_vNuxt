    "test": "vitest"
  },
  "devDependencies": {
    "@hypernym/nuxt-gsap": "^2.4.2",
    "@nuxt/devtools": "latest",
    "@nuxtjs/i18n": "^8.1.0",
    "@nuxtjs/sitemap": "^5.1.0",
    "@vite-pwa/nuxt": "^0.4.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.10.1",
    "nuxt-icon": "^0.6.8",
    "nuxt-og-image": "^2.2.4",
    "vite-svg-loader": "^5.1.0"
  },
  "dependencies": {
    "@nuxt/image": "^1.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/robots": "^3.0.0",
    "@vueuse/core": "^10.7.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
